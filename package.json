    "@types/jasminewd2": "2.0.10",
    "@types/node": "^18.19.64",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^19.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "typescript": "5.6.3"
  }
